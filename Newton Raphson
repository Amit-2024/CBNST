//newton Raphson

#include<stdio.h>
#include<math.h>

double fx(double x)
{
	return x*x*x*x-x-10;
}

double diff(double x)
{
	return 4*x*x*x-1;
}

int main()
{
	double a =1, b=2;
	double mid=(a+b)/2;
	while(1)
	{
	    b=mid-fx(mid)/diff(mid);
	    printf("%lf\n",b);
	    if(fabs(b-mid)<=0.0001)
	    break;
	    mid=b;
	}
}
